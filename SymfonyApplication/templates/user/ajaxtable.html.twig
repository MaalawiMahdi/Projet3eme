{% for user in users %}
    <tr>
        <td>{{ user.id }}</td>
        <td>{{ user.password }}</td>
        <td>{{ user.type }}</td>
        <td>{{ user.mail }}</td>
        <td>
            <a href="{{ path('user_edit', {'id': user.id}) }}">edit</a>
        </td>
    </tr>
{% else %}
    <tr>
        <td colspan="5">no records found</td>
    </tr>
{% endfor %}
