{% extends 'base.html.twig' %}

{% block title %}Societe index{% endblock %}

{% block body %}
    <h1>Societe index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Numregistre</th>
                <th>Adresse</th>
                <th>Type</th>
                <th>Etat</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for societe in societes %}
            <tr>
                <td>{{ societe.id }}</td>
                <td>{{ societe.numregistre }}</td>
                <td>{{ societe.adresse }}</td>
                <td>{{ societe.type }}</td>
                <td>{{ societe.etat ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('societe_show', {'id': societe.id}) }}">show</a>
                    <a href="{{ path('societe_edit', {'id': societe.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('societe_new') }}">Create new</a>
{% endblock %}
