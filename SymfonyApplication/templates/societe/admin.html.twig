

{% include('temp1.html.twig') %}
<div class="content">

    <h1>List des demandes</h1>

    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>Numregistre</th>
            <th>Adresse</th>
            <th>Type</th>
            <th>Etat</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for societe in liste_des_demandes %}
            <tr>
                <td>{{ societe.id }}</td>
                <td>{{ societe.numregistre }}</td>
                <td>{{ societe.adresse }}</td>
                <td>{{ societe.type }}</td>
                <td>{{ societe.etat ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('societe_show', {'id': societe.id}) }}">show</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
